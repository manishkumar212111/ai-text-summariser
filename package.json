{
  "name": "ai-text-summariser",
  "version": "1.0.4",
  "description": "TypeScript wrapper for Summarizer API with streaming support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manishkumar212111/ai-text-summariser.git"
  },
  "bugs": {
    "url": "https://github.com/manishkumar212111/ai-text-summariser/issues"
  },
  "homepage": "https://github.com/manishkumar212111/ai-text-summariser#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "summarizer",
    "text",
    "streaming"
  ],
  "author": "Manish Kumar",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^4.5.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
